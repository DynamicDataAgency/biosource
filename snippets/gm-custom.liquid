<script>
  var check = false;
  var ga_interval = setInterval(checkApp, 100);

  function checkApp() {
    if (typeof GloboMenuConfig == 'object' && $('.gm-menu').length) {
      check = true;
      clearInterval(ga_interval);
    }
    if (check) {
      $('.custom-image-1 img').attr('src', 'https://biosourcenutra.com/cdn/shop/files/1731437925_0.png?v=6213854456543438433');
      $('.custom-image-2 img').attr('src', 'https://biosourcenutra.com/cdn/shop/files/1731439325_0.png?v=15552186706469170476');
    }
  }
</script>